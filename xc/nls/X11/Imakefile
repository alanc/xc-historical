#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

INSTALLFLAGS = $(INSTDATFLAGS)

#ifndef BuildXimp
#define BuildXimp NO
#endif
#ifndef BuildXsi
#if !BuildXimp
#define BuildXsi YES
#endif
#endif

#if BuildXsi
SUBDIRS = Xsi
#endif

#if BuildXimp
SUBDIRS = Ximp
#endif /* BuildXsi */

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
