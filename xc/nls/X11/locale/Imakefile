XCOMM $XConsortium$
#define IHaveSubdirs
#define PassCDebugFlags

#ifdef XlibI18nPath
  XLIBI18N_PATH = XlibI18nPath
#else
  XLIBI18N_PATH = $(LIBDIR)/locale
#endif

   LOCALE_ALIAS = locale.alias
     LOCALE_DIR = locale.dir

        SUBDIRS = XLC_LOCALE Compose

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))

InstallNonExecFile($(LOCALE_ALIAS),$(XLIBI18N_PATH))
InstallNonExecFile($(LOCALE_DIR),$(XLIBI18N_PATH))
