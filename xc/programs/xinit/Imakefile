        DEFINES = ConnectionFlags -DBINDIR=\"$(BINDIR)\"
LOCAL_LIBRARIES = $(XLIB)
          SRCS1 = xinit.c 
          OBJS1 = xinit.o 
       PROGRAMS = xinit xstartup

ComplexProgramTarget_1(xinit,$(XLIB),)

xstartup:  xstartup.cpp
	$(RM) xstartup
	$(CPP) $(STD_SYS_DEFINES) <xstartup.cpp | \
		sed '/^#  *[0-9][0-9]*  *.*$$/d' >xstartup
	chmod a+x xstartup

InstallProgram(xstartup,$(BINDIR))
