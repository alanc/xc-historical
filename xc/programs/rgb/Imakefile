   SITE_RGB_DB = -DRGB_DB=\"DefaultRGBDatabase\"
      DEPLIBS = 
      DEFINES = NdbmDefines $(SITE_RGB_DB)
     INCLUDES = -I$(SERVERSRC)/include
 INSTALLFLAGS = $(INSTLIBFLAGS)
        SRCS1 = rgb.c
        OBJS1 = rgb.o
        SRCS2 = showrgb.c
        OBJS2 = showrgb.o
         SRCS = $(SRCS1) $(SRCS2)
         OBJS = $(OBJS1) $(OBJS2)
     PROGRAMS = rgb showrgb
    DATAFILES = rgb.pag rgb.dir
      DATADEP = rgb.dir		/* just want one run of rgb */
#ifndef SGIArchitecture
       DBMLIB = -ldbm
#endif
#if defined(SparcArchitecture) && HasGcc
           CC = cc
#endif

all:: $(PROGRAMS) $(DATADEP)

SingleProgramTarget(rgb,$(OBJS1),/**/,$(DBMLIB))
SingleProgramTarget(showrgb,$(OBJS2),/**/,$(DBMLIB))
SpecialObjectRule(rgb.o,$(ICONFIGFILES), /**/)
SpecialObjectRule(showrgb.o,$(ICONFIGFILES), /**/)
InstallMultiple($(DATAFILES) rgb.txt,$(LIBDIR))
InstallProgramWithFlags(showrgb,$(BINDIR),)

$(DATADEP):  rgb rgb.txt
	$(RM) $(DATAFILES)
	./rgb rgb < rgb.txt

clean::
	$(RM) $(DATAFILES)

DependTarget()
