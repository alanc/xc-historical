xkb_keymap "ibmpc default"

xkb_keycodes		{ include "ibmpc"	};
xkb_types		{ include "default"	};
xkb_compatibility	{ include "default"	};
xkb_symbols { 
    include "us"	
    override key <DELE> {
	type= "CTRL+ALT",
	symbols[Group1]= [ Delete, Terminate_Server ]
    };
};
