#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = \
	bitmap oclock twm xauth xbiff xcalc xclipboard xclock xcmap \
 	xditview xdm xdpyinfo xedit xev xfd xhost xinit xkill \
 	xload xlogo xlsclients xlsfonts xlswins xmag xman xmessage xmh \
	xmodmap xpr xprop xrdb xrefresh \
 	xset xsetroot xterm xwd xwininfo xwud

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
