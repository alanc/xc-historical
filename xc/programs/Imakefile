#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = \
	appres bitmap listres oclock twm xauth xbiff xcalc xclipboard xclock \
	xditview xdm xdpyinfo xedit xfd xhost xinit xkill \
 	xload xlogo xlsclients xlsfonts xlswins xmag xman xmessage xmh \
	xmodmap xpr xprop xrdb xrefresh \
 	xset xsetroot xstdcmap xterm xwd xwininfo xwud

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
