#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = \
	bitmap pseudoroot uwm x10tox11 xbiff xcalc xclock xdm xdpyinfo xedit \
	xev xfd xhost xinit xkill xload xlogo xlsfonts xlswins xmag xman xmh \
	xmodmap xpr xprop xrdb xrefresh xset xsetroot xterm xwd xwininfo xwud

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
