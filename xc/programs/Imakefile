#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = bitmap uwm x10tox11 xbiff xcalc xclock xdpyinfo xedit xev xfd \
	xhost xinit xkill xload xlogo xlsfonts xlswins xmag xmh xmodmap xpr \
	xprop xpseudo xrdb xrefresh xset xsetroot xterm xwd xwininfo xwud

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
