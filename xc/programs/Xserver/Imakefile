saber_src_sun:
	XCOMM cd dix
	XCOMM make saber_src
	XCOMM cd ../mi
	XCOMM make saber_src
	XCOMM cd ../mfb
	XCOMM make saber_src
	XCOMM cd ../cfb
	XCOMM make saber_src
	XCOMM cd ../sun
	XCOMM make saber_src
	XCOMM cd ../../os/4.2bsd
	XCOMM make saber_src
	XCOMM cd ../../../extensions/server
	XCOMM make saber_src
	XCOMM cd ../../fonts/mkfontdir
	XCOMM make saber_src_server
	XCOMM cd ../../lib/Xau
	XCOMM make saber_src
	XCOMM cd ../../server
	XCOMM load /usr/lib/libm.a
	XCOMM load /usr/lib/libdbm.a
#endif /* dosunServer */



