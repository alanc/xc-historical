OURDIX = dir_dix
OURDDX = dir_ddx_mfb dir_ddx_cfb dir_ddx_mi
OUROS = dir_os_42bsd
DECDDX = dir_ddx_dec_lk201 dir_ddx_dec_qdss dir_ddx_dec_qvss dir_ddx_dec_ws dir_ddx_dec_mfbpmax dir_ddx_dec_cfbpmax
HPDDX = dir_ddx_hp dir_ddx_hpMit
OTHERDDX = dir_ddx_apollo dir_ddx_apollo_apc dir_ddx_ibm dir_ddx_macII dir_ddx_sun dir_ddx_tek dir_ddx_omron dir_ddx_mips

SUBDIRS = $(OURDIX) $(OURDDX) $(OUROS) $(DECDDX) $(HPDDX) $(OTHERDDX)

     ALLDDXDIRS = ddx/mi ddx/cfb ddx/mfb \
                  ddx/dec/lk201 ddx/dec/qvss ddx/dec/qdss ddx/dec/qdss/libtl \
                  ddx/sun \
                  ddx/apollo ddx/apollo/apc \
                  ddx/ibm \
		  ddx/dec/ws ddx/dec/mfbpmax ddx/dec/cfbpmax \
                  ddx/hp ddx/hp.mit ddx/tek \
                  ddx/macII ddx/omron ddx/mips
      ALLOSDIRS = os/4.2bsd
     ALLSUBDIRS = dix $(ALLDDXDIRS) $(ALLOSDIRS)

InstallMultipleFlags($(ALL),$(BINDIR),$(INSTPGMFLAGS))

#define IHaveSubdirs

DependSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(SUBDIRS))
LintSubdirs($(SUBDIRS))

NamedCleanSubdirs(AllClean,$(ALLSUBDIRS))
NamedTagSubdirs(AllTags,$(ALLSUBDIRS))

MakeMakeSubdirs($(ALLSUBDIRS),AllMakefiles)



$(EXTDIR) $(SUBDIRS): FRC
	@cd $@ ; echo "making all in $(CURRENT_DIR)/$@..."; \
	$(MAKE) $(MFLAGS) PassCDebugFlags all

clean::
	$(RM) $(ALLPOSSIBLE)

InstallManPage(Xserver,$(MANDIR))

install::
	@echo "************************************************************"
	@echo "* "
	@echo "*     Do not forget to create a link called $(BINDIR)/X"
	@echo "*     pointing to the default server, if there is one."
	@echo "* "
	@echo "************************************************************"

FRC:
