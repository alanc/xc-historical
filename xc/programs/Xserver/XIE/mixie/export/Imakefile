		mecphoto.o \
		medraw.o \
		medrawp.o \
		melut.o \
		mephoto.o \
		meroi.o

   INCLUDES = -I../include -I../../include -I../../../../../include/xie \
              -I../../../include -I../../../../../include  \
              -I../../../../../../include -I../../../../../../server/include \
              -I$(INCLUDESRC)

SubdirLibraryRule($(OBJS))
NormalLibraryObjectRule()
