XCOMM $XConsortium$
#include <Server.tmpl>

#define IHaveSubdirs

     DIXIEDIR = dixie
        DIXIE = $(DIXIEDIR)/libdixie.a

     DDXIEDIR = ddxie/mi
        DDXIE = $(DDXIEDIR)/libddxie.a

     SUBDIRS = $(DIXIEDIR) $(DDXIEDIR)
	
     SUBOBJS = $(DIXIE) $(DDXIE)
		

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(SUBDIRS))
LintSubdirs($(SUBDIRS))
