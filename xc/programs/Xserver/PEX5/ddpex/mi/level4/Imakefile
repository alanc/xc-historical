#include <Server.tmpl>

#ifndef PexDdpexCDebugFlags
#define PexDdpexCDebugFlags ServerCDebugFlags
#endif

#-D defines for ddpex:
#	DDTEST turns on some fprintf(stderr...)s for debugging

    DEFINES = PexDdpexDefines
CDEBUGFLAGS = PexDdpexCDebugFlags

  PEXSERVINC = ../../../include
  PEXINCLUDE = ../../../../../include/PEX
DDPEXINCLUDE = ../include

INCLUDES = -I. \
	-I$(DDPEXINCLUDE) \
	-I$(PEXINCLUDE) \
	-I$(INCLUDESRC) \
	-I$(PEXSERVINC) \
	-I$(SERVERSRC)/include

SRCS =  miPick.c \
	miSC.c \
	miStruct.c \
	miTraverse.c \
	miWks.c \
	miDynamics.c \
	css_plain.c \
	css_ex_str.c \
	css_tbls.c

OBJS = miPick.o \
	miSC.o \
	miStruct.o \
	miTraverse.o \
	miWks.o \
	miDynamics.o \
	css_plain.o \
	css_ex_str.o \
	css_tbls.o

NormalLibraryObjectRule()

NormalLibraryTarget(ddpex4,$(OBJS))

LintLibraryTarget(dp4, $(SRCS))
NormalLintTarget($(SRCS))

DependTarget()

