    INCLUDES = -I. -I$(PEXSRVINC) -I$(PEXINCLUDE) -I$(INCLUDESRC) \
		-I$(SERVERSRC)/include


SRCS =  convUtil.c cOCprim.c uOCprim.c floatconv.c check.c \
	OCattr.c OCcolour.c convReq.c uconvRep.c Convert.c \
