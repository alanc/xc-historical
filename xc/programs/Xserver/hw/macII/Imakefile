	macIIKMap.o

CDEBUGFLAGS = ServerCDebugFlags
   INCLUDES = -I.  -I../../include -I../mfb -I../../dix -I../../../include
     LFLAGS = -buxz

NormalLibraryObjectRule()

all:: $(OFILES) 

NormalLibraryTarget(macII,$(OBJS))
DependTarget()
InstallManPage(XmacII,$(MANDIR))

lint: ${CFILES} ${LIBCFILES}
	$(LINT) $(LFLAGS) -ClibmacII $(INCLUDES) $(SRCS) >lint.out 2>&1
