XCOMM $XConsortium$
#include <Server.tmpl>
#define IHaveSubdirs


SUBDIRS = XF86Vga256Drivers

SRCS = */?*.c 

OBJS = */?*_drv.o 

all:: subdirs libdriver256.a

NormalLibraryObjectRule()

NamedMakeSubdirs(subdirs,$(SUBDIRS))

InstallLinkKitLibrary(driver256,$(LINKKITDIR)/drivers)

NormalDepLibraryTarget(driver256,$(SUBDIRS),$(OBJS))

DependSubdirs($(SUBDIRS))

InstallLinkKitNonExecFile(Imakefile,$(LINKKITDIR)/drivers/vga256)

#if XF86LinkKit
#include <LinkKit.tmpl>
#endif
