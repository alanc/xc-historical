#include <Server.tmpl>

#define IHaveSubdirs

SUBDIRS=ngle input

SRCS = \
	ddx_info.c \
	hpCursorUtils.c \
	hpInit.c
	
OBJS = \
	ddx_info.o \
	hpCursorUtils.o \
	hpInit.o \
    	ngle/ngle.o \
	input/libhp.a

LOBJS = \
	ddx_info.ln \
	hpCursorUtils.ln \
	hpInit.ln

DEFINES = $(ANSI_DEFINES)

INCLUDES = -I. -I./include -I./input -I../mfb -I../cfb -I../mi \
	-I../../include -I$(INCLUDESRC) -I../../../extensions/include

LINTLIBS = ../../dix/llib-ldix.ln ../../os/4.2bsd/llib-los.ln \
	../mfb/llib-lmfb.ln ../mi/llib-lmi.ln ../cfb/llib-lcfb.ln

NormalLibraryObjectRule()
NormalRelocatableTarget(hp,$(OBJS))
DependTarget()

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))


