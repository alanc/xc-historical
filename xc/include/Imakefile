#define IHaveSubdirs
#define PassCDebugFlags /**/

INSTALLFLAGS = $(INSTINCFLAGS)

#if BuildFresco
FRESCOSUBDIR = Fresco
#endif

REGSUBDIRS = bitmaps extensions fonts

SUBDIRS = $(REGSUBDIRS) $(FRESCOSUBDIR)

HEADERS = DECkeysym.h \
	Sunkeysym.h \
	X.h \
	XWDFile.h \
	Xatom.h \
	Xfuncproto.h \
	Xfuncs.h \
	Xmd.h \
	Xos.h \
	Xosdefs.h \
	Xproto.h \
	Xprotostr.h \
	Xthreads.h \
	Xw32defs.h \
	ap_keysym.h \
	keysym.h \
	keysymdef.h 

all::

BuildIncludes($(HEADERS),.,.)

InstallMultiple($(HEADERS),$(INCDIR))
MakeSubdirs($(SUBDIRS))

depend::

clean::
