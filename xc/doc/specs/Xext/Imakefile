MakeSimpleDoc(shape,shape.ms)

MakeSimpleDoc(xtest,xtest.ms)

MakeSimpleDoc(mit-shm,mit-shm.ms)

MakeSimpleDoc(bigreq,bigreq.ms)

MakeSimpleDoc(xc-misc,xc-misc.ms)

MakeTblDoc(record,record.ms)

MakeTblDoc(buffer,buffer.ms)

#if HasLatex

#ifndef LatexCmd
#define LatexCmd latex
#endif
#ifndef DvipsCmd
#define DvipsCmd dvips
#endif

LATEX = LatexCmd
DVIPS = DvipsCmd

#ifndef MakeLatexDoc
#define MakeLatexDoc(file,srcs)						@@\
all:: file.PS								@@\
									@@\
file.dvi: srcs								@@\
	$(LATEX) srcs							@@\
									@@\
file.PS: file.dvi							@@\
	$(DVIPS) -o file.nPS file && $(MV) file.nPS $@			@@\
									@@\
clean::									@@\
	RemoveFiles(file.nPS file.PS file.dvi file.aux file.log)
#endif /* MakeLatexDoc */

MakeLatexDoc(sync,sync.tex)

#endif
