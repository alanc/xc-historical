#include <Server.tmpl>
#define	IHaveSubdirs

SUBDIRS = di os
LBXLIBS = di/libdilbx.a os/libos.a $(LIBSRC)/lbxutil/liblbxutil.a 
OTHERLIBS = $(XEXTLIB) $(XLIB) $(XAULIB) $(XDMCPLIB) $(EXTRA_LIBRARIES)

ServerTarget(lbxproxy,$(SUBDIRS),NullParameter,$(LBXLIBS),$(OTHERLIBS))

DependSubdirs($(SUBDIRS))

ForceSubdirs($(SUBDIRS))
