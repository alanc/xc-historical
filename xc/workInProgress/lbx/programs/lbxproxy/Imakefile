#include <Server.tmpl>
#define	IHaveSubdirs

SUBDIRS = di os
LBXLIBS = di/libdilbx.a os/libos.a $(LIBSRC)/lbxutil/liblbxutil.a 
SYSLIBS = $(XEXTLIB) $(XLIB) $(OS) $(XAULIB) $(XDMCPLIB)

ServerTarget(lbxproxy,$(SUBDIRS),NullParameter,$(LBXLIBS),$(SYSLIBS))

DependSubdirs($(SUBDIRS))

ForceSubdirs($(SUBDIRS))
