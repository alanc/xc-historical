#include <Server.tmpl>

#define	IHaveSubdirs

SUBDIRS = lzw delta
OBJS = lzw/?*.o delta/?*.o
DONES = lzw/DONE delta/DONE

#undef _LinkBuildLibrary
#define _LinkBuildLibrary(lib) LinkBuildLibrary(lib)

NormalDepLibraryTarget(lbxutil,$(SUBDIRS) $(DONES),$(OBJS))

ForceSubdirs($(SUBDIRS))

DependSubdirs($(SUBDIRS))
