SRCS = Berklib.c
OBJS = Berklib.o

#undef _LinkBuildLibrary
#define _LinkBuildLibrary(lib) LinkBuildLibrary(lib)
NormalLibraryTarget(Xbsd,$(OBJS))
InstallLibrary(Xbsd,$(USRLIBDIR))

InstallLinkKitLibrary(Xbsd,$(LINKKITDIR)/lib)

DependTarget()
