      CCOPTIONS = LibraryCCOptions
    STD_DEFINES = LibraryDefines
    CDEBUGFLAGS = LibraryCDebugFlags
        DEFINES = -DSHAPE
       INCLUDES = -I. -I$(TOOLKITSRC) -I$(TOP) -I$(TOP)/X11
   INSTALLFLAGS = $(INSTINCFLAGS)
       LINTLIBS = $(LINTXLIB) $(LINTXTOOL)

