#define IHaveSubdirs
#define PassCDebugFlags

#ifndef NeedBerklib
#define NeedBerklib NO
#endif

#if NeedBerklib
BERKDIR = Xbsd
#endif

#if BuildFresco
FRESCODIRS = Fresco Xtf
#endif

#if BuildLBX
LBXDIR = lbxutil
#endif

LINTSUBDIRS = xtrans Xau Xdmcp X11 oldX ICE SM Xext Xt Xmu Xaw XIE Xi \
	      Xtst font FS PEX5

SUBDIRS = $(BERKDIR) $(LINTSUBDIRS) $(FRESCODIRS) $(LBXDIR)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(LINTSUBDIRS))
MakeLintSubdirs($(LINTSUBDIRS),install.ln,install.ln)
