#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

#
# Be sure to build the toolkit after the old resource manager, because
# the clients under the toolkit use it.
#

SUBDIRS = X \
	oldX \
	oldXrm \
	oldXtk \
	Xt

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(SUBDIRS))
