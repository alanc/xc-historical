#define DoNormalLib NormalLibICE
#define DoSharedLib SharedLibICE
#define DoDebugLib DebugLibICE
#define DoProfileLib ProfileLibICE
#define LibName ICE
#define SoRev SOICEREV
#define IncSubdir ICE

#if (SystemV || SystemV4) && !HasSockets
CONN_DEFINES = -DTCPCONN
#else
CONN_DEFINES = ConnectionFlags
#endif

#if HasBSD44Sockets
SOCK_DEFINES = -DBSD44SOCKETS
#endif

HEADERS = ICE.h \
          ICEproto.h \
          ICElib.h \
          ICElibint.h

SRCS = accept.c \
       authutil.c \
       connect.c \
       connutil.c \
       error.c \
       getauth.c \
       iceauth.c \
       listen.c \
       misc.c \
       ping.c \
       process.c \
       protosetup.c \
       register.c \
       replywait.c \
       setauth.c \
       shutdown.c \
       watch.c

OBJS = accept.o \
       authutil.o \
       connect.o \
       connutil.o \
       error.o \
       getauth.o \
       iceauth.o \
       listen.o \
       misc.o \
       ping.o \
       process.o \
       protosetup.o \
       register.o \
       replywait.o \
       setauth.o \
       shutdown.o \
       watch.o

#include <Library.tmpl>

SpecialCLibObjectRule(connect,$(ICONFIGFILES),$(CONN_DEFINES))
SpecialCLibObjectRule(listen,$(ICONFIGFILES),$(CONN_DEFINES))
SpecialCLibObjectRule(misc,$(ICONFIGFILES),$(CONN_DEFINES))
SpecialCLibObjectRule(connutil,$(ICONFIGFILES),$(CONN_DEFINES) $(SOCK_DEFINES))

DependTarget()
