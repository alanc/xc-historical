#define DoNormalLib NormalLibICE
#define DoSharedLib SharedLibICE
#define DoDebugLib DebugLibICE
#define DoProfileLib ProfileLibICE
#define LibName ICE
#define SoRev SOICEREV
#define IncSubdir ICE

DEFINES = -DICE

#if (SystemV || SystemV4) && !HasSockets
CONN_DEFINES = -DTCPCONN
#else
CONN_DEFINES = ConnectionFlags
#endif

#if HasBSD44Sockets
SOCK_DEFINES = -DBSD44SOCKETS
#endif

DEPEND_DEFINES = $(TRANS_INCLUDES) $(CONN_DEFINES)

HEADERS = ICE.h \
          ICEproto.h \
          ICElib.h \
          ICElibint.h

SRCS = accept.c \
       authutil.c \
       connect.c \
       error.c \
       getauth.c \
       iceauth.c \
       listen.c \
       locking.c \
       misc.c \
       ping.c \
       process.c \
       protosetup.c \
       register.c \
       replywait.c \
       setauth.c \
       shutdown.c \
       transport.c \
       watch.c

OBJS = accept.o \
       authutil.o \
       connect.o \
       error.o \
       getauth.o \
       iceauth.o \
       listen.o \
       locking.o \
       misc.o \
       ping.o \
       process.o \
       protosetup.o \
       register.o \
       replywait.o \
       setauth.o \
       shutdown.o \
       transport.o \
       watch.o

#include <Library.tmpl>

SpecialCLibObjectRule(transport,$(ICONFIGFILES),$(TRANS_INCLUDES) $(CONN_DEFINES) $(SOCK_DEFINES))
LinkSourceFile(transport.c,$(TRANSCOMMSRC))

DependTarget()
