ks_tables.h: $(INCLUDESRC)/keysymdef.h util/makekeys.c
	$(RM) $@ makekeys
	-$(CC) -o makekeys util/makekeys.c $(CFLAGS) $(LOCAL_LDFLAGS) $(LDLIBS) $(EXTRA_LOAD_FLAGS)
	./makekeys < $(INCLUDESRC)/keysymdef.h > ks_tables_h
	$(MV) ks_tables_h $@
	$(RM) makekeys ks_tables_h

includes:: ks_tables.h des.h $(AUTHSRCS) $(DESSRCS) $(LCSRCS)

depend:: ks_tables.h des.h $(AUTHSRCS) $(DESSRCS) $(LCSRCS)

clean::
	$(RM) ks_tables_h ks_tables.h makekeys

clean::
	$(RM) $(AUTHSRCS) $(DESSRCS) $(LCSRCS)

DependTarget()


XCOMM structure alignment errors can safely be ignored.
XCOMM complaints about qevent can be ignored too.

NormalLintTarget(*.c)
