    STD_DEFINES = LibraryDefines
    CDEBUGFLAGS = LibraryCDebugFlags
       INCLUDES = -I. -I$(XLIBSRC) -I$(TOP) -I$(TOP)/X11
   INSTALLFLAGS = -m 0644
       LINTLIBS = $(LINTXLIB)

HEADERS = \
	Atoms.h \
	Cardinal.h \
	Convert.h \
	Intrinsic.h \
	Misc.h \
	Shell.h \
	TopLevel.h \
	TM.h

SRCS = \
	Alloc.c \
	ArgList.c \
	Callback.c \
	Composite.c \
	Constraint.c \
	Convert.c \
	Converters.c \
	Core.c \
	Create.c \
	Cursors.c \
	Destroy.c \
	Error.c \
	Event.c \
	GCManager.c \
	Geometry.c \
	Initialize.c \
	Intrinsic.c \
	Manage.c \
	NextEvent.c \
	Popup.c \
	Resources.c \
	Shell.c \
	TMparse.c \
	TMstate.c \
	Vendor.c

OBJS = \
	Alloc.o \
	ArgList.o \
	Callback.o \
	Composite.o \
	Constraint.o \
	Convert.o \
	Converters.o \
	Core.o \
	Create.o \
	Cursors.o \
	Destroy.o \
	Error.o \
	Event.o \
	GCManager.o \
	Geometry.o \
	Initialize.o \
	Intrinsic.o \
	Manage.o \
	NextEvent.o \
	Popup.o \
	Resources.o \
	Shell.o \
	TMparse.o \
	TMstate.o \
	Vendor.o

#if DebugLibXt && ProfileLibXt
DebuggedAndProfiledLibraryObjectRule()
#else
# if DebugLibXt
DebuggedLibraryObjectRule()
# else
#  if ProfileLibXt
ProfiledLibraryObjectRule()
#  else
NormalLibraryObjectRule()
#  endif
# endif
#endif

NormalLibraryTarget(Xt,$(OBJS))
AliasedLibraryTarget(Xt,Xt11)
LintLibraryTarget(Xt,$(SRCS))
InstallLibrary(Xt,$(USRLIBDIR))
InstallLibraryAlias(Xt,Xt11,$(USRLIBDIR))
InstallLintLibrary(Xt,$(LINTLIBDIR))

#if ProfileLibXt
ProfiledLibraryTarget(Xt,$(OBJS))
AliasedLibraryTarget(Xt_p,Xt11_p)
InstallLibrary(Xt_p,$(USRLIBDIR))
InstallLibraryAlias(Xt_p,Xt11_p,$(USRLIBDIR))
#endif

#if DebugLibXt
DebuggedLibraryTarget(Xt,$(OBJS))
AliasedLibraryTarget(Xt_d,Xt11_d)
#endif

InstallMultiple($(HEADERS),$(INCDIR))

DependTarget()

NormalLintTarget($(SRCS))
