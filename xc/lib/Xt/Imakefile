    STD_DEFINES = LibraryDefines
    CDEBUGFLAGS = LibraryCDebugFlags
       INCLUDES = -I. -I$(XLIBSRC) -I$(TOP) -I$(TOP)/X11
   INSTALLFLAGS = -m 0644
       LINTLIBS = $(LINTXLIB)

HEADERS = \
