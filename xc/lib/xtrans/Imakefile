DEFINES = ConnectionFlags -DTEST -DTHISHOST=\"`hostname`\"
XCOMM	DEFINES = -DTCPCONN -DUNIXCONN -DTEST -DTHISHOST=\"`hostname`\"
XCOMM	CDEBUGFLAGS = -g
XCOMM	SYS_LIBRARIES = -ldbmalloc
LOCAL_LIBRARIES = XawClientLibs

OBJS = transport.o
SRCS = transport.c

AllTarget(ProgramTargetName(clienttest))
AllTarget(ProgramTargetName(servtest))
AllTarget(ProgramTargetName(displaytest))
AllTarget(ProgramTargetName(bindanytest))
AllTarget(ProgramTargetName(parsetest))

NormalProgramTarget(clienttest,$(OBJS) clienttest.o,NullParameter,NullParameter,NullParameter)
NormalProgramTarget(servtest,$(OBJS) servtest.o,NullParameter,NullParameter,NullParameter)
CenterProgramTarget(servtest,servtest.c $(SRCS),servtest.o $(OBJS),NullParameter,NullParameter)
NormalProgramTarget(displaytest,displaytest.o,NullParameter,-lX11,NullParameter)
NormalProgramTarget(bindanytest,$(OBJS) bindanytest.o,NullParameter,NullParameter,NullParameter)
NormalProgramTarget(parsetest,$(OBJS) parsetest.o,NullParameter,NullParameter,NullParameter)

DependTarget()
BuildIncludesTop(Xtrans.h)
