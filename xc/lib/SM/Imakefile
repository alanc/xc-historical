#define DoNormalLib NormalLibSM
#define DoSharedLib SharedLibSM
#define DoDebugLib DebugLibSM
#define DoProfileLib ProfileLibSM
#define LibName SM
#define SoRev SOSMREV
#define IncSubdir SM

#ifdef SharedSMReqs
REQUIREDLIBS = SharedSMReqs
#endif

#if (SystemV || SystemV4) && !HasSockets
CONN_DEFINES = -DTCPCONN
#else
CONN_DEFINES = ConnectionFlags
#endif

#if HasBSD44Sockets
SOCK_DEFINES = -DBSD44SOCKETS
#endif

HEADERS = SM.h \
          SMproto.h \
          SMlib.h \
          SMlibint.h

SRCS = sm_client.c \
       sm_manager.c \
       sm_process.c \
       sm_error.c \
       sm_misc.c \
       sm_auth.c

OBJS = sm_client.o \
       sm_manager.o \
       sm_process.o \
       sm_error.o \
       sm_misc.o \
       sm_auth.o

#include <Library.tmpl>

SpecialCLibObjectRule(sm_manager,$(ICONFIGFILES),$(CONN_DEFINES))

DependTarget()
