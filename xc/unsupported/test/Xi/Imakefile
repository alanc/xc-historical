   LOCLIBS = $(XILIB) $(XLIB)
   DEPLIBS = $(DEPXILIB) $(DEPXLIB)
   SRCS = XTouch.c XPErrors.c XList.c XSelInput.c XSendEv.c

all: xtouch xprotoerrors xlist xselinput xsendev

NormalProgramTarget(xtouch,XTouch.o,$(DEPLIBS),$(LOCLIBS), /**/)
NormalProgramTarget(xprotoerrors,XPErrors.o,$(DEPLIBS),$(LOCLIBS), /**/)
NormalProgramTarget(xlist,XList.o,$(DEPLIBS),$(LOCLIBS), /**/)
NormalProgramTarget(xselinput,XSelInput.o,$(DEPLIBS),$(LOCLIBS), /**/)
NormalProgramTarget(xsendev,XSendEv.o,$(DEPLIBS),$(LOCLIBS), /**/)

DependTarget()
