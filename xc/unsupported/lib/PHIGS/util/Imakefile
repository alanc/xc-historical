#include <Library.tmpl>

#ifndef PexPhigsCDebugFlags
#define PexPhigsCDebugFlags LibraryCDebugFlags
#endif

  CCOPTIONS = PexCCOptions
CDEBUGFLAGS = PexPhigsCDebugFlags
    DEFINES = PexPhigsDefines

SRCS = ut_path.c ut_mat.c ut_misc.c ut_vers.c utx.c utx_conv.c ut_ntfy.c \
       ut_htab.c
OBJS = ut_path.o ut_mat.o ut_misc.o ut_vers.o utx.o utx_conv.o ut_ntfy.o \
       ut_htab.o

.SUFFIXES: .o .lint

   PEXAPIINC = -I../include
  PEXINCLUDE = -I../../../include/PEX
    INCLUDES = $(PEXAPIINC) $(PEXINCLUDE) -I../../../../X11

LINTOPTS = -uv
LINT_LIBS = ../pex/llib-lpex.ln
XLINT_LIBS = $(LINTXLIB)

all:: DONE

DONE: ${OBJS}
	touch DONE

extract : $(SRCS)

lint:   $(SRCS)
	$(LINT) $(SRCS) $(LINTFLAGS) $(LINT_LIBS) 

lint-x: $(SRCS)
	$(LINT) $(SRCS) $(LINTFLAGS) $(LINT_LIBS) $(XLINT_LIBS) 

lintlib : llib-lut.ln

llib-lut.ln : $(SRCS)
	$(LINT) $(SRCS) $(LINTFLAGS) -Cut 

.c.lint:
	$(LINT) $< $(LINTFLAGS) $(LINT_LIBS) $(XLINT_LIBS) 

#ifdef HPArchitecture
SpecialObjectRule(hpext.o, $(IRULESRC)/$(MACROFILE), $(EXT_DEFINES))
#endif

DependTarget()

clean::
	$(RM) DONE
