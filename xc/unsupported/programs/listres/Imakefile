DEPLIBS = $(DEPXAWLIB) $(DEPXMULIB) $(DEPXTOOLLIB) $(DEPEXTENSIONLIB) $(DEPXLIB)
LOCAL_LIBRARIES = $(XAWLIB) $(XMULIB) $(XTOOLLIB) $(EXTENSIONLIB) $(XLIB)
SYS_LIBRARIES = -lm
SRCS = listres.c util.c 
OBJS = listres.o util.o 
HEADERS = $(AWIDGETSRC)\/*.h $(TOOLKITSRC)\/*.h

depend:: widgets.h

clean:: 
	$(RM) widgets.h

ComplexProgramTarget(listres)
InstallAppDefaults(Listres)

widgets.h: genlist.sh
	$(RM) $@
	sh genlist.sh $(HEADERS) >$@
