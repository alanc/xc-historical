XCOMM $XConsortium$
#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'
        XDVISRC = libXdvi
        XDVILIB = $(XDVISRC)/libXdvi.a
     DEPXDVILIB = $(XDVILIB)
        DEPLIBS = $(DEPXDVILIB) XawClientDepLibs
LOCAL_LIBRARIES = $(XDVILIB) XawClientLibs
  SYS_LIBRARIES = -lm
          SRCS1 = xditview.c
          OBJS1 = xditview.o
          SRCS2 = xtotroff.c
          OBJS2 = xtotroff.o
       PROGRAMS = xditview xtotroff
        SUBDIRS = libXdvi

MakeSubdirs($(SUBDIRS))

ComplexProgramTarget_1(xditview,$(LOCAL_LIBRARIES),NullParameter)
SingleProgramTarget(xtotroff,$(OBJS2),$(LOCAL_LIBRARIES),NullParameter)
InstallAppDefaults(Xditview)

DependSubdirs($(SUBDIRS))
