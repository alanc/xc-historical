#ifdef MacIIArchitecture
      IEEE_DEFS = -DIEEE
#endif /* MacIIArchitecture */

        DEFINES = $(IEEE_DEFS) $(SIGNAL_DEFINES)
        DEPLIBS = XawClientDepLibs
LOCAL_LIBRARIES = XawClientLibs
  SYS_LIBRARIES = -lm
           SRCS = actions.c math.c xcalc.c 
           OBJS = actions.o math.o xcalc.o 

