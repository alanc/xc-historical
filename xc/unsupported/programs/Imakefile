#define IHaveSubdirs
#define PassCDebugFlags CDEBUGFLAGS="$(CDEBUGFLAGS)"

#if BuildPhigs
PHIGSDIRS = auto_box beach_ball gpc
#endif

#if BuildFresco
FRESCODIRS = dish i2mif ixx fdraw
#endif

SUBDIRS = beforelight ico listres maze puzzle showfont viewres xbiff xcalc \
	xcmstest xditview xedit xev xeyes xfontsel xgas xgc xload \
	xman xpr $(PHIGSDIRS) $(FRESCODIRS)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
