SRCS = 6x10.bdf 6x12.bdf 6x13.bdf 8x13.bdf 8x13bold.bdf 9x15.bdf cursor.bdf 
OBJS = 6x10.snf 6x12.snf 6x13.snf 8x13.snf 8x13bold.snf 9x15.snf cursor.snf 


MakeFonts()

FontTarget(6x10)
FontTarget(6x12)
FontTarget(6x13)
FontTarget(8x13)
FontTarget(8x13bold)
FontTarget(9x15)
FontTarget(cursor)

InstallFonts ($(FONTDIR)/misc)

depend::
	@echo "skipping make depend in bdf/misc"
