#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

#if BuildPexExt
PEXDIRS = PEX
#endif

#if BuildFonts
FONTDIRS = bdf scaled
#endif

/* tools must be built before font dirs */
        SUBDIRS = lib tools $(FONTDIRS) $(PEXDIRS)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
