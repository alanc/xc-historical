#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

#if BuildPexExt
PEXDIRS = PEX
#endif

#ifndef BuildBdfFonts
#define BuildBdfFonts YES
#endif

#if BuildBdfFonts
BDFDIRS = bdf
#endif

/* tools must be built before bdf */
        SUBDIRS = lib tools $(BDFDIRS) $(PEXDIRS)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
